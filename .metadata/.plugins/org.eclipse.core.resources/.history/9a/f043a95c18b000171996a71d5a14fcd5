package demoExpertSystem;

import java.util.ArrayList;

// note: each rule currently allows only one consequent
public class Rule {

	private ArrayList<String> antecedents;
	private String consequent;
	private String logicType;
	
	//constructor
	public Rule(ArrayList<String> newAntecedents,
				String newConsequent,
				String newLogicType){
		
		antecedents = newAntecedents;
		consequent = newConsequent;
		logicType = newLogicType;
	}
	
	public ArrayList<String> getAntecedents(){
		
		return antecedents;
	}
	
	public String getConsequent(){
		return consequent;
	}
	
	public String getLogicType(){
		return logicType;
	}
	
}
